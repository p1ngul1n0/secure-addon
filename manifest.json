{
  "manifest_version": 2,

  "name": "Web Checker",
  "description": "Check se!",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "storage","tabs","cookies","*://*/*"
   ],
   "content_scripts": [
   {
     "matches": ["*://*/*"],
     "js": ["content.js"]
   }
 ]
}