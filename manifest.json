{
  "manifest_version": 2,

  "name": "Header Checker",
  "description": "Check the headers of a website!",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab","storage","tabs"
   ],
    "content_scripts": [
   {
     "matches": ["*://*/*"],
     "js": ["content.js"]
   }
 ]
}